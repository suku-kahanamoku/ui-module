{
  "name": "@suku-kahanamoku/ui-module",
  "version": "1.0.8",
  "description": "UI module",
  "repository": "@suku-kahanamoku/ui-module",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./types": {
      "types": "./dist/runtime/types.d.ts",
      "import": "./dist/runtime/types.d.ts"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build --failOnWarn=false",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run test && npm run prepack && changelogen --release && npm publish --access public && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.3",
    "@nuxt/ui": "^4.0.1",
    "@suku-kahanamoku/common-module": "latest",
    "@suku-kahanamoku/lang-module": "latest",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.37.0",
    "nuxt": "^4.1.3",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}
